  </Reaction>

  <Reaction name = "STIM_dim_3" id="STIM_dim_3">
    <Reactant specieID="STIMCaER" power="2"/>
    <Product specieID="STIMCaER_STIMCaER"/>
    <forwardRate>10e-4</forwardRate>
    <reverseRate>5e-5</reverseRate>
