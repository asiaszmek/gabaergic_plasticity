    <Product specieID="STIMCaER"/>
    <forwardRate>0.1e-3</forwardRate>
    <reverseRate>5000e-3</reverseRate>
  </Reaction>

  <Reaction name = "STIMCaER_2" id="STIMCaER_2">
    <Reactant specieID="STIMCaER_STIM" />
    <Reactant specieID="CaER" />
    <Product specieID="STIMCaER_STIMCaER"/>
    <forwardRate>0.1e-3</forwardRate>
    <reverseRate>5000e-3</reverseRate>
  </Reaction>
  <Reaction name = "STIMCaER_3" id="STIMCaER_3">
