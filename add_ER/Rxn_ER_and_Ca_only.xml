<!-- CaM, CaMCa4 and PP2B Binding -->
<!--1) CaM  + 2Ca
<-> CaMCa2 -->
<!--This is the C site-->
<Reaction name="CaMC_bind" id="CaMC_bind">
  <Reactant specieID="CaM" />
  <Reactant specieID="Ca" n="2" />
  <Product specieID="CaMCa2C" />
  <forwardRate>6e-6</forwardRate>
  <reverseRate>9.1e-3</reverseRate>
  <Q10>0.2</Q10>
</Reaction>
<!--2) CaMCa2  + 2Ca
<-> CaMCa4 -->
